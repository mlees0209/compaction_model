	author = {Ojha, Chandrakanta and Werth, Susanna and Shirzaei, Manoochehr},
	year = {2019},
	keywords = {GPS, InSAR, GRACE, Drought, Groundwater depletion, Groundwater levels},
	pages = {3127--3143},
	file = {Ojha et al_2019_Groundwater Loss and Aquifer System Compaction in San Joaquin Valley During.pdf:/Users/mlees/Dropbox/ZoteroFilesInDropbox/Ojha et al_2019_Groundwater Loss and Aquifer System Compaction in San Joaquin Valley During.pdf:application/pdf;Snapshot:/Users/mlees/Zotero/storage/U3854G4C/2018JB016083.html:text/html}
