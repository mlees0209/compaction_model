	url = {https://doi.org/10.1029/2017WR022250},
	doi = {10.1029/2017wr022250},
	abstract = {The accelerated rate of decline in groundwater levels across California's Central Valley results from overdrafting and low rates of natural recharge and is exacerbated by droughts. The lack of observations with an adequate spatiotemporal resolution to constrain the evolution of groundwater resources poses severe challenges to water management efforts. Here we present SAR interferometric measurements of high-resolution vertical land motion across the valley, revealing multiscale patterns of aquifer hydrogeological properties and groundwater storage change. Investigating the depletion and degradation of the aquifer-system during 2007-2010, when the entire valley experienced a severe drought, we find that {\textasciitilde}2\% of total aquifer-system storage was permanently lost, owing to irreversible compaction of the system. Over this period, the seasonal groundwater storage change amplitude of 10.11 ± 2.5 km 3 modulates a long-term groundwater storage decline of 21.32 ± 7.2 km 3. Estimates for subbasins show more complex patterns, most likely associated with local hydrogeology, recharge, demand, and underground flow. Presented measurements of aquifer-system compaction provide a more complete understanding of groundwater dynamics and can potentially be used to improve water security.},
	number = {7},
	journal = {Water Resources Research},
	author = {Ojha, Chandrakanta and Farr, Tom G. and Werth, Susanna and Shirzaei, Manoochehr and Argus, Donald F.},
	year = {2018},
	pages = {4449--4460},
	file = {Ojha et al_2018_Sustained Groundwater Loss in California's Central Valley Exacerbated by.pdf:/Users/mlees/Dropbox/ZoteroFilesInDropbox/Ojha et al_2018_Sustained Groundwater Loss in California's Central Valley Exacerbated by.pdf:application/pdf}
}

@techreport{puskas_hydrologic_2017,
	title = {Hydrologic {Loading} {Model} {Displacements} from the {National} and {Global} {Data} {Assimilation} {Systems} ({NLDAS} and {GLDAS})},
	url = {https://www.unavco.org/data/gps-gnss/associated-products/hydrological/displacement-model-readme.pdf},
	abstract = {Loading and Poroelastic Effects Water in the Earth is stored in oceans, lakes, rivers, groundwater reservoirs, as well as in snowpack, soil, and vegetation. In continental interiors, groundwater and surface water predominate. The mass of water varies seasonally and is large enough to produce displacements of several mm in GPS time series (Herring et al., 2016). The water mass also varies geographically, depending on climate (Fovell and Fovell, 1993) and local geology (Miller, 1994). These mass variations are part of the water cycle. Ground deformation changes in water storage can divided into elastic and poroelastic components. Elastic deformation occurs from water mass changes at the Earth's surface from water bodies (lakes, rivers, etc.) and from snowpack, soil, and vegetation. When there is more precipitation during winter or the rainy season, the water goes into these storage media (Figure 1). The weight of the extra water depresses the ground surface, which deforms elastically in response to the load. In the summer or dry season when evaporation and runoff exceeds precipitation, surface water mass decreases and the earth rebounds, producing uplift (Wahr et al., 2013; van Dam et al., 2001; Meertens et al., 2011). Poroelastic deformation is produced by changes in groundwater storage (Figure 2). Groundwater is stored in aquifers, or formation of porous and permeable sediments and rocks, and aquitards, or formations of low­permeability silts and clay (Miller, 1994). When water is withdrawn from an aquifer or aquitard, pore spaces that were previously supported by water pressure are compacted and overlying ground surface subsides (Galloway et al., 1999). When aquifers are recharged from precipitation or seasonal streamflow, pores open again and the ground surface rises. In aquitards, the compaction is permanent and the resulting subsidence is permanent. Groundwater pumping drives poroelastic deformation in agricultural regions},
	author = {Puskas, Christine M and Meertens, Charles M and Phillips, David},
	year = {2017},
	file = {Puskas et al_2017_Hydrologic Loading Model Displacements from the National and Global Data.pdf:/Users/mlees/Dropbox/ZoteroFilesInDropbox/Puskas et al_2017_Hydrologic Loading Model Displacements from the National and Global Data.pdf:application/pdf}
}

